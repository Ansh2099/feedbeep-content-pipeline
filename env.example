# NewsData.io API Configuration
NEWSDATA_API_KEY=your_newsdata_api_key_here
NEWSDATA_BASE_URL=https://newsdata.io/api/1/news

# AI Service Configuration (Choose one)
GEMINI_API_KEY=your_gemini_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Supabase Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Application Configuration
LOG_LEVEL=info
MAX_ARTICLES_PER_FETCH=10
AI_MODEL=geminipro # or gpt-4

# Content Quality Settings
MIN_CONTENT_QUALITY_SCORE=50
ENABLE_CONTENT_QUALITY_CHECK=true
ENABLE_SCRAPER_FALLBACK=true

# Rate Limiting
NEWSDATA_RATE_LIMIT=10
NEWSDATA_RATE_WINDOW=60000
GEMINI_RATE_LIMIT=60
GEMINI_RATE_WINDOW=60000

# Scraper Settings
SCRAPER_TIMEOUT=10000
SCRAPER_ENABLED=true 